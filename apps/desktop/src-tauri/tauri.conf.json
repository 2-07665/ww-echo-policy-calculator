{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Echo Calculator",
  "version": "2.0.0",
  "identifier": "com.echo.calculator",
  "build": {
    "frontendDist": "../ui"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "main",
        "title": "声骸强化策略计算器",
        "width": 940,
        "height": 880,
        "minWidth": 940,
        "minHeight": 700,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": false,
    "icon": [
      "icons/icon.ico",
      "icons/icon.png",
      "icons/icon.icns"
    ]
  }
}
