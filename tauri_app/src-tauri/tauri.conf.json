{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Echo Calculator",
  "version": "0.1.0",
  "identifier": "com.echo.calculator",
  "build": {
    "frontendDist": "../ui"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "main",
        "title": "鸣潮声骸强化策略计算器",
        "width": 940,
        "height": 880,
        "minWidth": 940,
        "minHeight": 700,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["app"],
    "icon": []
  }
}
